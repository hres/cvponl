<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
   
<hibernate-mapping>
       
    <class name="ca.gc.hc.model.lookups.ReportSource" table="SOURCE_LX">
    	<id name="reportSourceID" column="SOURCE_LX_ID" unsaved-value="null">
    	    <generator class="native"/>
    	</id>
    	<property name="reportSourceCode" column="SOURCE_CODE"/>
    	<property name="reportSourceNameE" column="EN_DESC"/>
    	<property name="reportSourceNameF" column="FR_DESC"/>    	
    </class>

</hibernate-mapping>   