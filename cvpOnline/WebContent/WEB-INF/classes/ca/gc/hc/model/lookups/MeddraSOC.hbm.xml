<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
   
<hibernate-mapping>
       
    <class name="ca.gc.hc.model.lookups.MeddraSOC" table="MEDDRA_SOC_LX">
    	<id name="socCode" column="SOC_CODE" unsaved-value="null">
    	    <generator class="native"/>
    	</id>
    	<property name="socNameE" column="SOC_NAME_ENG"/>
    	<property name="socNameF" column="SOC_NAME_FR"/>
    	<property name="meddraVersion" column="MEDDRA_VERSION"/>    	
    </class>

</hibernate-mapping>   