<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
   
<hibernate-mapping>
       
    <class name="ca.gc.hc.model.ReportReactionMV" table="ADR_MV">
    	<id name="adrID" column="ADR_ID" unsaved-value="null">
    	    <generator class="native"/>
    	</id>
    	<property name="reportID" column="REPORT_ID"/>    	
    	<property name="reportNumber" column="REPORT_NO"/>
    	<property name="versionNumber" column="VERSION_NO"/>
