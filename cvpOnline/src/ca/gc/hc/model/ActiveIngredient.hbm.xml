<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
   
<hibernate-mapping>
       
    <class name="ca.gc.hc.model.ActiveIngredient" table="ACTIVE_INGREDIENTS">
    	<id name="activeIngredientID" column="ACTIVE_INGREDIENT_ID" unsaved-value="null">
    	    <generator class="native"/>
    	</id>
    	<property name="activeIngredientName" column="ACTIVE_INGREDIENT_NAME"/>    	
    	<property name="primaryIngredientID" column="PRIMARY_INGREDIENT_ID"/>
    </class>

</hibernate-mapping>   