<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
   
<hibernate-mapping>
       
    <class name="ca.gc.hc.model.lookups.Outcome" table="OUTCOME_LX" mutable="false">
    	<id name="outcomeID" column="OUTCOME_LX_ID" unsaved-value="null">
    	    <generator class="native"/>
    	</id>
    	<property name="outcomeCode" column="OUTCOME_CODE"/>
    	<property name="outcomeNameE" column="EN_DESC"/>
    	<property name="outcomeNameF" column="FR_DESC"/>    	
    	<property name="lastUpdateDate" column="L_UPDATE_DT" type="date"/>    	    	
    </class>

</hibernate-mapping>   